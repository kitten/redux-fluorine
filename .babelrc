{
  "presets": [
    "babel-preset-philpl"
  ],
  "ignore": [
    "/node_modules/"
  ],
  "plugins": [
    "closure-elimination"
  ],
  env: {
    cjs: {
      plugins: [
        "transform-es2015-modules-commonjs"
      ]
    },
    es: {
      plugins: [
        "./scripts/babel-use-lodash-es"
      ]
    }
  }
}
